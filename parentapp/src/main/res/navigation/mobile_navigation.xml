<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.dodolz.kiddos.navigation.HomeFragment"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_app_detail"
        android:name="com.dodolz.kiddos.navigation.AppDetailFragment"
        tools:layout="@layout/fragment_detail" />

    <fragment
        android:id="@+id/nav_video"
        android:name="com.dodolz.kiddos.navigation.VideoFragment"
        tools:layout="@layout/fragment_video" />

    <fragment
        android:id="@+id/nav_setting"
        android:name="com.dodolz.kiddos.navigation.SettingFragment"
        tools:layout="@layout/fragment_setting" />

    <fragment
        android:id="@+id/nav_location"
        android:name="com.dodolz.kiddos.navigation.LocationFragment"
        tools:layout="@layout/fragment_location" />

    <activity
        android:id="@+id/nav_edit_profile"
        android:name="com.dodolz.kiddos.navigation.EditProfileActivity"
        tools:layout="@layout/activity_edit_profile" />

    <activity
        android:id="@+id/nav_add_child"
        android:name="com.dodolz.kiddos.navigation.AddChildActivity"
        tools:layout="@layout/activity_add_child"/>

</navigation>